<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dct="http://purl.org/dc/terms/" xmlns:adms="http://www.w3.org/ns/adms#" xmlns:cc="hhttp://creativecommons.org/ns#" xmlns:schema="http://schema.org/" xmlns:dcat="http://www.w3.org/ns/dcat#" xmlns:vann="http://purl.org/vocab/vann/" xmlns:dadk="http://data.gov.dk/model/vocabular/modelcat#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:owl="https://www.w3.org/2002/07/owl#">
    <xs:import namespace="hhttp://creativecommons.org/ns#" schemaLocation="ccxml"/>
    <xs:import namespace="http://data.gov.dk/model/vocabular/modelcat#" schemaLocation="dadkxml"/>
    <xs:import namespace="http://purl.org/vocab/vann/" schemaLocation="vannxml"/>
    <xs:import namespace="http://schema.org/" schemaLocation="schemaxml"/>
    <xs:import namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#" schemaLocation="skema"/>
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xmlxml"/>
    <xs:import namespace="http://www.w3.org/ns/adms#" schemaLocation="admsxml"/>
    <xs:import namespace="http://www.w3.org/ns/dcat#" schemaLocation="dcatxml"/>
    <xs:import namespace="http://xmlns.com/foaf/0.1/" schemaLocation="foafxml"/>
    <xs:import namespace="https://www.w3.org/2002/07/owl#" schemaLocation="owlxml"/>
    <xs:element name="description">
        <xs:complexType mixed="true">
            <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="issued">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NMTOKEN">
                    <xs:attribute ref="rdf:datatype" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="modified">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NMTOKEN">
                    <xs:attribute ref="rdf:datatype" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="publisher">
        <xs:complexType>
            <xs:attribute ref="rdf:resource" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="title">
        <xs:complexType mixed="true">
            <xs:attribute ref="xml:lang" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="type">
        <xs:complexType>
            <xs:attribute ref="rdf:resource" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="license">
        <xs:complexType>
            <xs:attribute ref="rdf:resource" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="format">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute ref="rdf:datatype" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="rights" type="xs:string"/>
</xs:schema>
