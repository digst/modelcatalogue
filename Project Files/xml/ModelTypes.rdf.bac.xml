  <!-- Classes -->
<owl:Class rdf:about="http://data.gov.dk/model/vocabulary/ModelTypes#Vocabulary">
    <rdfs:label xml:lang="en">Vocabulary</rdfs:label>
    <rdfs:label xml:lang="da">Vokabular</rdfs:label>
    <rdfs:comment xml:lang="en">core model, which...</rdfs:comment>
    <rdfs:comment xml:lang="da">kernemodel, der baseres på en beskrivelsesramme i RDF, og som sikrer semantisk entydighed, sammenhæng og maskintilgængelighed</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://rdfs.org/ns/void#Dataset"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
    <rdfs:subClassOf rdf:resource="http://data.gov.dk/model/vocabulary/ModelTypes#CoreModel"/>
    
<owl:equivalentClass rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
 <!-- dette indtil videre  kopi af voafs restriction -->
	   <!-- The following restriction defines the class in a recursive way : a voaf:Vocabulary must rely on or be used by at least one other voaf:Vocabulary 
       The recursivity starting point is VOAF itself-->
<owl:unionOf rdf:parseType="Collection">
      <owl:Restriction>
        <owl:minCardinality>1</owl:minCardinality>
        <owl:onProperty rdf:resource="http://purl.org/vocommons/voaf#reliesOn"/>
      </owl:Restriction>
      <owl:Restriction>
        <owl:minCardinality>1</owl:minCardinality>
        <owl:onProperty rdf:resource="http://purl.org/vocommons/voaf#usedBy"/>
      </owl:Restriction>
</owl:unionOf>
<rdfs:isDefinedBy rdf:resource="http://purl.org/vocommons/voaf"/>	
<vs:term_status>stable</vs:term_status>
</owl:Class>
