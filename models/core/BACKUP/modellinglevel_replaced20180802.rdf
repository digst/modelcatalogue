   <rdf:Description rdf:about="https://data.gov.dk/model/core/modellinglevel#Dissemination">
      <dct:identifier rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Dissemination</dct:identifier>
      <rdf:type rdf:resource="http://purl.org/net/provenance/ns#DataItem"/>
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://data.gov.dk/model/core/modellinglevel#ModelingLevels"/>
      <dct:title xml:lang="en">Dissemination</dct:title>
      <dct:description xml:lang="da">Niveau 1: Formidling: udgøres af et mindre antal basale regler, som handler om grundlæggende dokumentation af modellen, tilføjelse af metadata, et ensartet grafisk udtryk, samt at modellen er udstillet på internettet.</dct:description>
      <skos:definition xml:lang="da">Niveau 1: Formidling: udgøres af et mindre antal basale regler, som handler om grundlæggende dokumentation af modellen, tilføjelse af metadata, et ensartet grafisk udtryk, samt at modellen er udstillet på internettet.</skos:definition>
      <dct:description xml:lang="en">Level 1: Dissemination consists of a minimum number of rules concerning basic documentation of the model, the addition of meta data, a graphical notation and publication.</dct:description>
      <skos:definition xml:lang="en">Level 1: Dissemination consists of a minimum number of rules concerning basic documentation of the model, the addition of meta data, a graphical notation and publication.</skos:definition>
      <skos:prefLabel xml:lang="da">Niveau 1: Formidling</skos:prefLabel>
      <skos:prefLabel xml:lang="en">Level 1: Dissemination</skos:prefLabel>
      <dcat:theme rdf:resource="tbd"/>
      <adms:status rdf:resource="http://inspire.ec.europa.eu/registry/status/valid"/>
   </rdf:Description>

   <rdf:Description rdf:about="https://data.gov.dk/model/core/modellinglevel#Reuse">
      <dct:identifier rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Reuse</dct:identifier>
      <rdf:type rdf:resource="http://purl.org/net/provenance/ns#DataItem"/>
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://data.gov.dk/model/core/modellinglevel#ModelingLevels"/>
     <dct:title xml:lang="en">Reuse</dct:title>
      <dct:description xml:lang="da">Niveau 2: Genbrug: tillægger yderligere regler, som vedrører genbrug af modelelementer fra eksisterende modeller, højere kvalitet i definitioner og betegnelser samt anvendelse af et standardiseret udvekslingsformat.</dct:description>
      <skos:definition xml:lang="da">Niveau 2: Genbrug: tillægger yderligere regler, som vedrører genbrug af modelelementer fra eksisterende modeller, højere kvalitet i definitioner og betegnelser samt anvendelse af et standardiseret udvekslingsformat.</skos:definition>
      <dct:description xml:lang="en">Level 2: Reuse appends additional rules concerning the reuse of model elements from existing models, high quality of definitions and terms as well as applying a standardized exchange format.</dct:description>
      <skos:definition xml:lang="en">Level 2: Reuse appends additional rules concerning the reuse of model elements from existing models, high quality of definitions and terms as well as applying a standardized exchange format.</skos:definition>
      <skos:prefLabel xml:lang="da">Niveau 2: Genbrug</skos:prefLabel>
      <skos:prefLabel xml:lang="en">Level 2: Reuse</skos:prefLabel>
      <dcat:theme rdf:resource="tbd"/>
      <adms:status rdf:resource="http://inspire.ec.europa.eu/registry/status/valid"/>
   </rdf:Description>

   <rdf:Description rdf:about="https://data.gov.dk/model/core/modellinglevel#Cohesion">
